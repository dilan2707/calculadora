    private void jButton25ActionPerformed(java.awt.event.ActionEvent evt) {                                          
this.segundonumero=Float.parseFloat(this.casilla.getText());   
switch(this.operador){
    case "+":this.casilla.setText(sincero(this.primernumero+this.segundonumero));break;
    case "-":this.casilla.setText(sincero(this.primernumero-this.segundonumero));break;
    case "/":if(this.segundonumero==0){this.casilla.setText("math error :x");}
    else{
        this.casilla.setText(Float.toString(this.primernumero/this.segundonumero));}break;
    case "*":this.casilla.setText(sincero(this.primernumero*this.segundonumero));break;
    case "^":this.casilla.setText(sincero((float) Math.pow(this.primernumero,this.segundonumero)));break;
    case "âˆš":this.casilla.setText(sincero((float) Math.pow(this.primernumero,1/this.segundonumero)));break;
    case "iva":this.casilla.setText(sincero((float) (this.primernumero*0.19)));break;
    case "cos":this.casilla.setText(sincero((float) Math.cos(this.primernumero)));break;
    case "sin":this.casilla.setText(sincero((float) Math.sin(this.primernumero)));break;
    case "tan":this.casilla.setText(sincero((float) Math.tan(this.primernumero)));break;
}
    }                                         

